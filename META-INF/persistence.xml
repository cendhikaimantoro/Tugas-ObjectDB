<?xml version="1.0" encoding="UTF-8"?>
<persistence version="1.0" xmlns="http://java.sun.com/xml/ns/persistence" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/persistence  http://java.sun.com/xml/ns/persistence/persistence_1_0.xsd">
  <persistence-unit name="my-pu">
    <description>My Persistence Unit</description>
    <mapping-file>META-INF/mappingFile.xml</mapping-file>
    <class>entity.Branch</class>
    <class>entity.Card</class>
    <class>entity.Customer</class>
    <class>entity.Component</class>
    <class>entity.Employee</class>
    <class>entity.ExecutiveCustomer</class>
    <class>entity.Good</class>
    <class>entity.GoodInBranch</class>
    <class>entity.HQ</class>
    <class>entity.IDCard</class>
    <class>entity.Location</class>
    <class>entity.NonHQ</class>
    <class>entity.Order</class>
    <class>entity.OrderQty</class>
    <class>entity.RegularCustomer</class>
    <class>entity.OrderQty</class>
    <properties>
      <property name="javax.persistence.jdbc.url" value="objectdb://localhost:6136/PDL.odb"/>
      <property name="javax.persistence.jdbc.user" value="admin"/>
      <property name="javax.persistence.jdbc.password" value="admin"/>
    </properties>
  </persistence-unit>
</persistence>
